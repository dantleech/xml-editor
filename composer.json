{
    "minimum-stability": "dev",
    "name": "phpactor/xml-query",
    "description": "jQuery-like API for editing XML documents",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "require": {
        "phpunit/phpunit": "^7.2@dev",
        "phpactor/test-utils": "^1.0@dev",
        "phpstan/phpstan-shim": "^0.9.0@dev"
    },
    "require-dev": {
        "infection/infection": "dev-master",
        "microsoft/tolerant-php-parser": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Phpactor\\XmlQuery\\": "lib/",
            "Phpactor\\XmlQuery\\Bridge\\TolerantParser\\": "bridge/tolerant-parser/lib/",
            "Phpactor\\XmlQuery\\Bridge\\WorseReflection\\": "bridge/worse-reflection/lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phpactor\\XmlQuery\\Tests\\": "tests/",
            "Phpactor\\XmlQuery\\Bridge\\TolerantParser\\Tests\\": "bridge/tolerant-parser/tests/",
            "Phpactor\\XmlQuery\\Bridge\\WorseReflection\\Tests\\": "bridge/worse-reflection/tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
